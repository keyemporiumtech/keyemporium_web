{
  "name": "keyemporium-web",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "proxy-local": "ng serve --proxy-config src/config/proxy/local/proxy.conf.json",
    "proxy-local-ssl": "ng serve --proxy-config src/config/ssl/proxy/local/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "proxy-prod": "ng serve --proxy-config src/config/proxy/prod/proxy.conf.json",
    "proxy-prod-ssl": "ng serve --proxy-config src/config/ssl/proxy/prod/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "proxy-test": "ng serve --proxy-config src/config/proxy/test/proxy.conf.json",
    "proxy-test-ssl": "ng serve --proxy-config src/config/ssl/proxy/test/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "build": "ng build",
    "build-local": "ng build --configuration=local --base-href ./",
    "build-prod": "ng build --prod --base-href ./",
    "build-test": "ng build --configuration=test --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib-kit": "barrelsby --directory \"src/app/kit/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-oauth-social": "barrelsby --directory \"src/app/oauth-social/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-rest": "barrelsby --directory \"src/app/rest/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "build::lib": "npm run lib-kit && npm run lib-oauth-social && npm run lib-rest",
    "serve": "npm run build::lib && npm run start",
    "serve::local": "npm run build::lib && npm run proxy-local",
    "serve::local-ssl": "npm run build::lib && npm run proxy-local-ssl",
    "serve::prod": "npm run build::lib && npm run proxy-prod",
    "serve::prod-ssl": "npm run build::lib && npm run proxy-prod-ssl",
    "serve::test": "npm run build::lib && npm run proxy-test",
    "serve::test-ssl": "npm run build::lib && npm run proxy-test-ssl",
    "build::local": "npm run build::lib && npm run build-local",
    "build::prod": "npm run build::lib && npm run build-prod",
    "build::test": "npm run build::lib && npm run build-test",
    "publish": "cd _scripts/deploy && ./deploy-local.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular-tree-component": "^8.5.2",
    "angular2-qrscanner": "^1.0.7",
    "angularx-qrcode": "^1.6.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-device-detector": "1.3.3",
    "ngx-markdown": "^8.2.1",
    "prettier": "^1.19.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.14.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "barrelsby": "^1.0.2",
    "codelyzer": "~4.5.0",
    "css-minify": "^1.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.14.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~3.2.4"
  },
  "peerDependencies": {
    "prettier": "^1.19.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ngx-markdown": "^8.2.1",
    "angular2-qrscanner": "^1.0.7",
    "angularx-qrcode": "^1.6.4",
    "angular-tree-component": "^8.5.2",
    "ng2-pdf-viewer": "^6.4.1",
    "ng2-file-upload": "^1.4.0",
    "@compodoc/compodoc": "^1.1.11",
    "css-minify": "^1.0.1",
    "ngx-device-detector": "1.3.3",
    "angularx-social-login": "2.2.1"
  }
}
