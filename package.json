{
  "name": "keyemporium-web",
  "version": "1.2.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng serve",
    "proxy-local": "npm run ng serve -- --proxy-config src/config/proxy/local/proxy.conf.json",
    "proxy-local-ssl": "npm run ng serve -- --proxy-config src/config/ssl/proxy/local/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "proxy-prod": "npm run ng serve -- --proxy-config src/config/proxy/prod/proxy.conf.json",
    "proxy-prod-ssl": "npm run ng serve -- --proxy-config src/config/ssl/proxy/prod/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "proxy-test": "npm run ng serve -- --proxy-config src/config/proxy/test/proxy.conf.json",
    "proxy-test-ssl": "npm run ng serve -- --proxy-config src/config/ssl/proxy/test/proxy.conf.json --ssl --ssl-key src/config/ssl/server.key --ssl-cert src/config/ssl/server.cert",
    "build": "npm run ng build",
    "build-local": "npm run ng build -- --configuration=local --base-href ./",
    "build-prod": "npm run ng build -- --configuration=production --base-href ./",
    "build-test": "npm run ng build -- --configuration=test --base-href ./",
    "test": "npm run ng test",
    "lint": "npm run ng lint",
    "prettier": "npx prettier --check src/**/*.{ts,html}",
    "fix::prettier": "npx prettier --write src/**/*.{ts,html}",
    "eslint": "npx eslint src/**/*.{ts,html}",
    "fix::eslint": "npx eslint --fix src/**/*.{ts,html}",
    "e2e": "ng e2e",
    "husky": "npx jest",
    "lib-kit": "barrelsby --directory \"src/app/kit/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-oauth-social": "barrelsby --directory \"src/app/oauth-social/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-rest": "barrelsby --directory \"src/app/rest/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-kit-lnx": "barrelsby --directory src/app/kit/ --delete -e '.*(^.*spec.*$).*' -n index.ts -q",
    "lib-oauth-social-lnx": "barrelsby --directory src/app/oauth-social/ --delete -e '.*(^.*spec.*$).*' -n index.ts -q",
    "lib-rest-lnx": "barrelsby --directory src/app/rest/ --delete -e '.*(^.*spec.*$).*' -n index.ts -q",
    "build::lib": "npm run lib-kit && npm run lib-oauth-social && npm run lib-rest",
    "build::lib-lnx": "npm run lib-kit-lnx && npm run lib-oauth-social-lnx && npm run lib-rest-lnx",
    "serve": "npm run build::lib && npm run start",
    "serve::local": "npm run build::lib && npm run proxy-local",
    "serve::local-ssl": "npm run build::lib && npm run proxy-local-ssl",
    "serve::prod": "npm run build::lib && npm run proxy-prod",
    "serve::prod-ssl": "npm run build::lib && npm run proxy-prod-ssl",
    "serve::test": "npm run build::lib && npm run proxy-test",
    "serve::test-ssl": "npm run build::lib && npm run proxy-test-ssl",
    "build::local": "npm run build::lib && npm run build-local",
    "build::prod": "npm run build::lib && npm run build-prod",
    "build::test": "npm run build::lib && npm run build-test",
    "build::local-lnx": "npm run build::lib-lnx && npm run build-local",
    "build::prod-lnx": "npm run build::lib-lnx && npm run build-prod",
    "build::test-lnx": "npm run build::lib-lnx && npm run build-test",
    "publish": "cd _scripts/deploy && ./deploy-local.sh",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "npm run fix::prettier",
      "npm run fix::eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.3",
    "@angular/common": "^15.2.3",
    "@angular/compiler": "^15.2.3",
    "@angular/core": "^15.2.3",
    "@angular/forms": "^15.2.3",
    "@angular/platform-browser": "^15.2.3",
    "@angular/platform-browser-dynamic": "^15.2.3",
    "@angular/router": "^15.2.3",
    "@babel/core": "^7.19.3",
    "@circlon/angular-tree-component": "^11.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/marked": "^4.0.7",
    "@types/pdfjs-dist": "^2.10.378",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.1",
    "angularx-qrcode": "^15.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.26.1",
    "crypto-js": "^3.1.9-1",
    "d3-array": "^3.2.1",
    "d3-brush": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "marked": "^4.2.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^6.4.0",
    "ngx-quill": "^20.0.1",
    "pdfjs-dist": "^2.5.207",
    "rxjs": "^7.8.0",
    "sass": "^1.56.1",
    "tslib": "^1.14.1",
    "worker-loader": "^3.0.7",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cdk": "^15.2.3",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.3",
    "@angular/language-service": "^15.2.3",
    "@compodoc/compodoc": "^1.1.11",
    "@cypress/schematic": "^2.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "barrelsby": "^1.0.2",
    "css-minify": "^1.0.1",
    "cypress": "latest",
    "d3-color": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "2.8.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.8.2"
  },
  "peerDependencies": {
    "@circlon/angular-tree-component": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "css-minify": "^1.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^6.4.0"
  }
}
